[groupcentosdns]
centosdns			ansible_host=192.168.98.12 

[groupcentosdns:vars]
ansible_port=22
ansible_user=vagrant
ansible_ssh_private_key_file=sshkeys/centosdns-key
ansible_ssh_common_args=-oIdentityFile=sshkeys/insecure_private_key

