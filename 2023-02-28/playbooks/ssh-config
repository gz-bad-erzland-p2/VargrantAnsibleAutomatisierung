Host centoswebserver
  HostName 192.168.86.11
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile sshkeys/centoswebserver-key
  IdentityFile sshkeys/insecure_private_key
  IdentitiesOnly yes
  LogLevel FATAL
 
