  HostName 192.168.98.11
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile playbooks/sshkeys/centosdb-key
  IdentityFile playbooks/sshkeys/insecure_private_key
  IdentitiesOnly yes
  LogLevel FATAL

